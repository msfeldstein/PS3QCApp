			<string>Which mode to render back-facing polygons in: wireframe, point, or solid</string>
			<key>name</key>
			<string>Back-Facing Polygon Mode</string>
			<key>menu</key>
			<array>
				<string>Textured</string>
				<string>Wireframe</string>
				<string>Points</string>
			</array>
		</dict>
