			<key>description</key>
			<string>Which mode to render front-facing polygons in, wireframe, point, or solid</string>
			<key>name</key>
			<string>Front-Facing Polygon Mode</string>
			<key>menu</key>
			<array>
				<string>Textured</string>
				<string>Wireframe</string>
				<string>Points</string>
			</array>
		</dict>
		<key>inputBackPolygonMode</key>
		<dict>
