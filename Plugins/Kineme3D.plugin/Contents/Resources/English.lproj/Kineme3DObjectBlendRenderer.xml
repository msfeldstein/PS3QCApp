<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>nodeAttributes</key>
	<dict>
		<key>category</key>
		<string>Kineme 3D</string>
		<key>categories</key>
		<array>
			<string>Kineme 3D</string>
		</array>
		<key>copyright</key>
		<string>Copyright 2008 Kosada Incorporated. All rights reserved.</string>
		<key>description</key>
		<string>This patch Renders a Blend of 2 3D Objects

http://kineme.net/</string>
		<key>name</key>
		<string>3D Object Blend Renderer</string>
	</dict>
	<key>inputAttributes</key>
	<dict>
		<key>inputObject</key>
		<dict>
			<key>description</key>
			<string>First Object to Blend</string>
			<key>name</key>
			<string>Input Object 1</string>
		</dict>
		<key>inputObject2</key>
		<dict>
			<key>description</key>
			<string>Second Object to Blend</string>
			<key>name</key>
			<string>Input Object 2</string>
		</dict>
		<key>inputAlpha</key>
		<dict>
			<key>description</key>
			<string>The amount to blend each object [0 to 1]</string>
			<key>name</key>
			<string>Object Blend Amount</string>
		</dict>

		<key>inputImage</key>
		<dict>
			<key>description</key>
			<string>First Object's Texture</string>
			<key>name</key>
			<string>Input Image 1</string>
		</dict>
		<key>inputImage2</key>
		<dict>
			<key>description</key>
			<string>Second Object's Texture</string>
			<key>name</key>
			<string>Input Image 2</string>
		</dict>


		<key>inputColor</key>
		<dict>
			<key>description</key>
			<string>Object Color</string>
			<key>name</key>
			<string>Color</string>
		</dict>
		<key>inputXPosition</key>
		<dict>
			<key>description</key>
			<string>Object's X Position</string>
			<key>name</key>
			<string>X Position</string>
		</dict>
		<key>inputYPosition</key>
		<dict>
			<key>description</key>
			<string>Object's Y Position</string>
			<key>name</key>
			<string>Y Position</string>
		</dict>
		<key>inputZPosition</key>
		<dict>
			<key>description</key>
			<string>Object's Z Position</string>
			<key>name</key>
			<string>Z Position</string>
		</dict>

		<key>inputRotateX</key>
		<dict>
			<key>description</key>
			<string>Rotation about the X axis</string>
			<key>name</key>
			<string>X Rotation</string>
		</dict>
		<key>inputRotateY</key>
		<dict>
			<key>description</key>
			<string>Rotation about the Y axis</string>
			<key>name</key>
			<string>Y Rotation</string>
		</dict>
		<key>inputRotateZ</key>
		<dict>
			<key>description</key>
			<string>Rotation about the Z axis</string>
			<key>name</key>
			<string>Z Rotation</string>
		</dict>

		<key>inputScaleX</key>
		<dict>
			<key>description</key>
			<string>Scale along the X axis</string>
			<key>name</key>
			<string>X Scale</string>
		</dict>
		<key>inputScaleY</key>
		<dict>
			<key>description</key>
			<string>Scale along the Y axis</string>
			<key>name</key>
			<string>Y Scale</string>
		</dict>
		<key>inputScaleZ</key>
		<dict>
			<key>description</key>
			<string>Scale along the Z axis</string>
			<key>name</key>
			<string>Z Scale</string>
		</dict>
		
		<key>inputNormals</key>
		<dict>
			<key>description</key>
			<string>Determines how to calculate normals of the object to be rendered</string>
			<key>name</key>
			<string>Normals</string>
			<key>menu</key>
			<array>
				<string>None</string>
				<string>Faceted</string>
				<string>Smooth</string>
			</array>
		</dict>
		
		<key>inputUseReferenceIndexes</key>
		<dict>
			<key>description</key>
			<string>When True, all geometry indexes come from object 1.  This helps when meshes tear on blend or have weird textures/normals.</string>
			<key>name</key>
			<string>Use Reference Indices</string>
		</dict>
		

	</dict>
</dict>
</plist>
