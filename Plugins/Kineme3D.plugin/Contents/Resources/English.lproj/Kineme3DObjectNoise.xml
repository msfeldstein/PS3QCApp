<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>nodeAttributes</key>
	<dict>
		<key>category</key>
		<string>Kineme 3D</string>
		<key>categories</key>
		<array>
			<string>Kineme 3D</string>
		</array>
		<key>copyright</key>
		<string>Copyright 2008 Kosada Incorporated. All rights reserved.</string>
		<key>description</key>
		<string>This patch adds varying amounts of noise to the vertices in an object.
Enable "Update Continuously" to get constantly changing noise patterns on objects.
		
http://kineme.net/</string>
		<key>name</key>
		<string>3D Object Noise</string>
	</dict>
	<key>inputAttributes</key>
	<dict>
		<key>inputObject</key>
		<dict>
			<key>description</key>
			<string>Object to Twist</string>
			<key>name</key>
			<string>Input Object</string>
		</dict>
		<key>inputXAmount</key>
		<dict>
			<key>description</key>
			<string>Amount of noise on the X axis</string>
			<key>name</key>
			<string>X Amount</string>
		</dict>
		<key>inputYAmount</key>
		<dict>
			<key>description</key>
			<string>Amount of noise on the Y axis</string>
			<key>name</key>
			<string>Y Amount</string>
		</dict>
		<key>inputZAmount</key>
		<dict>
			<key>description</key>
			<string>Amount of noise on the Z axis</string>
			<key>name</key>
			<string>Z Amount</string>
		</dict>
		<key>inputContinuous</key>
		<dict>
			<key>description</key>
			<string>Causes the object to get renoised each frame</string>
			<key>name</key>
			<string>Update Continuously</string>
		</dict>
		
	</dict>
	<key>outputAttributes</key>
	<dict>
		<key>outputObject</key>
		<dict>
			<key>description</key>
			<string>The Twisted Object</string>
			<key>name</key>
			<string>Output Object</string>
		</dict>
	</dict>
</dict>
</plist>
