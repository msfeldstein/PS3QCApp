<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>nodeAttributes</key>
	<dict>
		<key>category</key>
		<string>Kineme CV</string>
		<key>categories</key>
		<array>
			<string>Kineme CV</string>
		</array>
		<key>copyright</key>
		<string>Copyright 2009 Kosada Incorporated. All rights reserved.</string>
		<key>description</key>
		<string>This patch detects objects in the source OpenCV Image, based on the input Haar Cascade criteria.

http://kineme.net/</string>
		<key>name</key>
		<string>Apply Haar Cascade</string>
	</dict>
	<key>inputAttributes</key>
	<dict>
		<key>inputImage</key>
		<dict>
			<key>description</key>
			<string>OpenCV Image to search</string>
			<key>name</key>
			<string>CV Image</string>
		</dict>
		<key>inputCascade</key>
		<dict>
			<key>description</key>
			<string>Cascade Data to search by</string>
			<key>name</key>
			<string>Cascade Data</string>
		</dict>
		<key>inputImageScale</key>
		<dict>
			<key>description</key>
			<string>Initial Image scaling -- 1.3 - 3.5 are good values (larger values are faster, but miss smaller faces)</string>
			<key>name</key>
			<string>Initial Scale</string>
		</dict>

		<key>inputScaleFactor</key>
		<dict>
			<key>description</key>
			<string>Amount of scaling between passes.  1.1-1.6 are good values</string>
			<key>name</key>
			<string>Scale Delta</string>
		</dict>
		<key>inputMinNeighbors</key>
		<dict>
			<key>description</key>
			<string>Minimum number of neighbors for grouping.</string>
			<key>name</key>
			<string>Minimum Neighbors</string>
		</dict>
		<key>inputMinSize</key>
		<dict>
			<key>description</key>
			<string>Minimum Object size</string>
			<key>name</key>
			<string>Minimum Object Size</string>
		</dict>
		<key>inputCannyPruning</key>
		<dict>
			<key>description</key>
			<string>Use Canny Pruning to speed up search</string>
			<key>name</key>
			<string>Canny Pruning</string>
		</dict>

	</dict>
	<key>outputAttributes</key>
	<dict>
		<key>outputRects</key>
		<dict>
			<key>description</key>
			<string>Rectangles of detected objects (structure of XYWH structures)</string>
			<key>name</key>
			<string>Rect Structure</string>
		</dict>
	</dict>
</dict>
</plist>
